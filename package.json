    "body-parser": "^1.19.0",
    "concurrently": "^6.0.2",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "mongodb": "^3.6.6",
    "mongoose": "^5.12.5",
    "validator": "^13.6.0"
  },
  "engines": {
    "node": "14.15.3",
    "npm": "6.14.9"
  }
}
